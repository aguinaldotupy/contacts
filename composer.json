{
    "name": "tupy/contacts",
    "description": "",
    "keywords": [
        "tupy",
        "contacts"
    ],
    "homepage": "https://github.com/aguinaldotupy/contacts",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Aguinaldo Tupy",
            "email": "aguinaldo.tupy@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "~5.8.0|^6.0|^7.0",
        "illuminate/filesystem": "~5.8.0|^6.0|^7.0",
        "illuminate/database": "~5.8.0|^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Tupy\\Contacts\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tupy\\Contacts\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tupy\\Contacts\\ContactsServiceProvider"
            ],
            "aliases": {
                "Contacts": "Tupy\\Contacts\\ContactsFacade"
            }
        }
    }
}
